{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "first-wave/accordion",
  "version": "0.1.0",
  "title": "Accordion",
  "category": "theme",
  "icon": "feedback",
  "description": "A vertically stacked set of interactive headings that each reveal a section of content.",
  "supports": {
    "interactivity": true,
    "html": false,
    "align": ["wide", "full"],
    "spacing": {
      "padding": true,
      "margin": true
    },
    "color": {
      "background": true
    }
  },
  "attributes": {
    "isAccordion": {
      "type": "boolean",
      "default": true
    }
  },
  "providesContext": {
    "first-wave/accordion": "isAccordion"
  },
  "allowedBlocks": ["first-wave/pane"],
  "textdomain": "first-wave-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}
